<launch>

    <param name="use_sim_time" value="false" />
    <node pkg="move_base" type="move_base" respawn="false" name="move_base1" output="screen" clear_params="true" args="/map:=/prior_map /odom:=/state_estimation">
        <rosparam file="$(find rm_global_planner)/param/costmap_common_params.yaml" command="load" ns="global_costmap" />
        <rosparam file="$(find rm_global_planner)/param/costmap_common_params.yaml" command="load" ns="local_costmap" />
        <rosparam file="$(find rm_global_planner)/param/local_costmap_params.yaml" command="load" />
        <rosparam file="$(find rm_global_planner)/param/global_costmap_params.yaml" command="load" />
        <rosparam file="$(find rm_global_planner)/param/base_local_planner_params.yaml" command="load" />
        <rosparam file="$(find rm_global_planner)/param/global_planner_params.yaml" command="load" />
        <remap from="/cmd_vel" to="/cmd_vel_mobe_base" />
    </node>

    <!-- <node name = "map_server" pkg = "map_server" type = "map_server" args="/home/xmurcs/Desktop/rcs/src/state_estimation_module/FAST_LIO/PCD/map.yaml /map:=/prior_map"/> -->
    

</launch>
